config_version=5

[application]

config/name="Lost Howls"
run/main_scene="res://scene/MainGame.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Global="*res://scripts/global.gd"
Signals="*res://scripts/signals.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[dialogic]

directories/dch_directory={
"Ghostedtree": "res://Ghostedtree.dch"
}
directories/dtl_directory={
"Ghostedtree": "res://Ghostedtree.dtl"
}
layout/style_list=[]
layout/default_style="Default"
variables={}

[display]

window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/stretch/scale_mode="integer"
window/transparent_background=false

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

[file_customization]

folder_colors={
"res://art/": "red",
"res://music/": "orange"
}

